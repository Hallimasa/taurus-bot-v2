// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WarframeBuild {
  id String @id @default(cuid())
}

model Guild {
  id                      String  @id
  cetusAlertsChannel      String?
  gameAlertsChannel       String?
  archonHuntAlertsChannel String?
  orbVallisAlertsChannel  String?
  baroKiteerAlertsChannel String?
  nighwaveAlertsChannel   String?
}
